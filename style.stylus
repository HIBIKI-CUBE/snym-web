a
  text-decoration: none

header
  position fixed
  top 0
  align-items center
  justify-content space-between
  width 100vw
  padding calc(var(--base-size) / 6) calc(var(--base-size) / 2 +  env(safe-area-inset-left)) calc(var(--base-size) / 6) calc(var(--base-size) / 2 +  env(safe-area-inset-right))
  height calc(var(--base-size) + env(safe-area-inset-top))
  box-sizing border-box
  color var(--ui-text-color)
  background-color var(--ui-bg)
  vendor(backdrop-filter, blur(10px))
  z-index 1000

.header_logo
  display inline-block
  height 100%
  width auto
  object-fit contain

.header_title
  display inline-block
  position absolute
  top 0
  height var(--base-size)
  line-height var(--base-size)
  font-size calc(var(--base-size) / 2)
  width auto
  margin 0 0 0 0.5em

.header_button
  margin 0
  padding 0
  position fixed
  top 0
  right 0
  padding 0 calc(var(--base-size) / 2 +  env(safe-area-inset-right)) 0
  z-index 4000
  border none
  box-sizing border-box
  height var(--base-size)
  display inline-flex
  align-items center
  justify-content center
  background-color transparent
  animation-name derotate_button
  animation-duration 500ms
  vendor(animation-timing-function, ease-in-out)
  animation-delay 0s
  animation-fill-mode both

.header_button_svg
  height 60%
  transform translate(0, -2%)
  z-index 3000
  pointer-events none

.header_button_checkbox
  display none

@keyframes fold_navigation
  from
    transform translate(0%, 0%)
  to
    transform translate(100%, 0%)

@keyframes expand_navigation
  from
    transform translate(100%, 0%)
  to
    transform translate(0%, 0%)

@keyframes rotate_button
  0%
    transform translate(0, 0) rotate(45deg)
  50%
    transform translate(0, 0) rotate(270deg)
  100%
    transform translate(-70vw, 0) rotate(270deg)

@keyframes derotate_button
  0%
    transform translate(-70vw, 0) rotate(270deg)
  50%
    transform translate(0, 0) rotate(270deg)
  100%
    transform translate(0, 0) rotate(45deg)

.header_navigation
  display flex
  width 70vw
  position fixed
  top 0
  z-index 1001
  right 0
  animation-name fold_navigation
  animation-duration 300ms
  animation-timing-function cubic-bezier(.8,0,.7,.7)
  animation-fill-mode both

.header_navigation_list_items
  display inline-block
  width 100%
  height var(--base-size)
  line-height var(--base-size)
  margin 0
  padding 0
  border none
  text-align center
  font-size var(--base-size-vw)
  color var(--ui-text-color)
  background-color transparent

  &:hover
    @media screen and (min-width 600px)
      background-color var(--ui-bg-hover)

  &+&:before
    content ''
    position absolute
    width 1px
    top calc(var(--base-size) * 0.1)
    height calc(var(--base-size) * 0.8)
    display block
    background-color var(--ui-text-color)

  @media screen and (max-width 600px)
    &:first-child
      display:none

    &+&:nth-child(2):before
      content none

#header_button:checked ~ .header_navigation
  animation-name expand_navigation
  animation-duration 300ms
  animation-timing-function ease-in-out
  animation-delay 200ms
  animation-fill-mode both

#header_button:checked ~ .header_button
  animation-name rotate_button
  animation-duration 500ms
  vendor(animation-timing-function, ease-in-out)
  animation-delay 0s
  animation-fill-mode both

main
  margin-top calc(var(--base-size) * 1.5)
  width 100vw

article
  padding 0 10vw
  width 80vw

.main_works_article-section_iframe
  width 80vw
  height calc(80vw / 16 * 9)

.main_works_hero_image
  width 100%
  height calc(100vh - var(--base-size))
  object-fit contain

.main_team_avatar
  width 5vw
  @media screen and (max-width 680px)
    width 15vw
  height auto

footer
  width 100vw