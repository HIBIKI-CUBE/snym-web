@charset "utf-8";

:root
  --base-size 7vh
  --base-size-vw 1.2vw
  --font-size-large 2vw
  --font-size-medium 2vw
  --font-size-small 2vw
  --accent-color #FC0
  --ui-over-text-color #000
  --ui-over-bg #888
  --ui-text-color #FFF
  --ui-text-hover-color #000
  --ui-bg #CCC
  --ui-bg-hover #888
  --ui-bg-focus #888
  @supports(backdrop-filter blur(10px)) or (-webkit-backdrop-filter blur(10px)) or (-moz-backdrop-filter blur(10px))
    --ui-bg rgba(200,200,200,0.5)
  --main-text-color rgba(255,255,255,1)
  --main-bg rgba(255,255,255,1)
  --bg rgba(0,0,0,1)

@media screen and (max-width 680px)
  :root
    --base-size 12vw

vendor(prop, args)
  {prop} args
  -webkit-{prop} args
  -moz-{prop} args

vendorarg(prop, arg)
  {prop} arg
  {prop} -webkit-+arg
  {prop} -moz-+arg
